<br>
<h1>All Events</h1>

  <% @events.each do |event| %>
    <ul>
      <li>
          <span> <%= event.event_name %> </span>
          <span class= "px-4"> <%= event.event_location %> </span>
          <span class= "px-4"> <%= event.event_date %> </span>
      </li>
    </ul>
    <hr>
  <% end %>

<%= link_to "New Events", new_event_path %>
