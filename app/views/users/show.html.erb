<h1>My events</h1>

<ul>
  <% @events.each do |event| %>
    <li>
      <%= event.event_name %>
    </li>
    <li>
      <%= event.event_location %>
    </li>
    <li>
      <%= event.event_date %>
    </li>
  <% end %>
</ul>

<h2>Attended events</h2>
 <ol>
    <% current_user.attended_events.each do |event| %>
    <li>
      <%= event.event_name %>
    </li> 
    <% end %>
 </ol>